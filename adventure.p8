--ui

function draw_ui()
	camera()
	
	palt(0,false)
	palt(12,true)
	spr(32,110,2)
	spr(48,2,2)
	spr(33,110,8)
	spr(49,110,16)
	palt()
	print_outline("X"..p.keys,118,2)
	print_outline("X"..p.hearts,10,3)
	print_outline("X"..p.moneys,118,10)
	print_outline("X"..p.sword,118,17)

end

function print_outline(text,x,y)
	print(text,x-1,y,0)
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)	
end
