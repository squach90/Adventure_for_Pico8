--message

function init_msg()
	message={}
end


function create_msg(name,...)
	msg_title=name
	message={...}
end

function upadte_msg()
	if (btnp(❎)) then
		deli(message,1)
	end
end

function draw_msg()
if message[1] then
		local y=100
		if p.y%16>9 then
			y=20
		end
		--titre
		rectfill(7,y,
		11+#msg_title*4,y+7,2)
		print(msg_title,10,y+2,9)
		--message
		rectfill(3,y+8,124,y+24,4)
		rect(3,y+8,124,y+24,2)
		print(message[1],6,y+11,15)
	end
end
