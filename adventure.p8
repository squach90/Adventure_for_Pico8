--player

function create_player()
	p={
				x=6,y=4,
				ox=0,oy=0,
				start_ox=0,start_oy=0,
				anim_t=0,
				sprite=16,
				keys=0,
				hearts=3,
				moneys=0,
				rnd_chest=flr(rnd(2)) + 1,
				start_game=false,
				sword=0,
				inventory={},
				selecte=0
			}
end

function open_inventory()
	_draw = i_draw
	_update= i_update
end

function player_mouvement()
 newx = p.x
 newy = p.y
  if p.anim_t == 0 then
      newox = 0
      newoy = 0
      if btn(⬅️) then
          newx -= 1
          newox = 8
          p.flip=true
      elseif btn(➡️) then
          newx += 1
          newox = -8
          p.flip=false
      elseif btn(⬆️) then
          newy -= 1
          newoy = 8
      elseif btn(⬇️) then
          newy += 1
          newoy = -8
      end
      if (btnp(🅾️)) open_inventory()
  end
	interact(newx,newy)
	if check_flag(4,newx,newy) then
		p.y=23
		p.x=29
	else
		sfx(2)
	end
	
	if check_enemies(7,newx,newy) then
		if p.sword==1 then	
			mset(newx,newy,15)
			sfx(8)
		else
			p.hearts-=1
			mset(newx,newy,15)
			sfx(7)
		end
	else
		sfx(2)
	end
	
	
	if check_chest(3,newx,newy) then
		
		if rnd_chest()==2 then  
			p.moneys+=10
		else
			if p.keys==0 then
			sfx(2) 
			else 
				p.hearts+=1
				p.keys-=2
				sfx(5)
			end
		end
		mset(newx,newy,9)
		--remplace le bloc piece par de la terre
	else
		sfx(2)
	end
	
	if p.x==119 and p.y==2 then
		p.sword=1
		mset(newx,newy,12)
		sfx(9)
	end
	
	if check_moneys(6,newx,newy) then
		p.moneys+=1
		mset(newx,newy,15)
		--remplace le bloc piece par de la terre
	else
		sfx(2)
	end
	
	
	if check_flag(5,newx,newy) then
		p.y=2
		p.x=114
		sfx(6)
	else
		sfx(2)
	end
	
	if not check_flag(0,newx,newy) 
	and (p.x!=newx or p.y!=newy) then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
		p.start_ox=newox
		p.start_oy=newoy
		p.anim_t=1
	end
	
	--animation
	p.anim_t=max(p.anim_t-0.125,0)
	p.ox=p.start_ox*p.anim_t
	p.oy=p.start_oy*p.anim_t
	
	if p.anim_t>=0.5 then
		p.sprite=17
	else
		p.sprite=16
	end
end

function interact(x,y)
	if check_flag(1,x,y) then
		pick_up_key(x,y)
	elseif check_flag(2,x,y) 
	and p.keys>0 then
		open_door(x,y)
end
	if not start_game==true then
		create_msg("jeu",
		"bienvenue sur adur.","attend un peu tu ne sais pas\ncomment jouer","pour te deplacer \nc'est ⬅️➡️⬆️⬇️","pour interagir c'est ❎","amuse toi bien 😐")
		start_game=true
	end
	if x==2 and y==22 then
		create_msg("louis",
		"bonjour repis, petit conseille \nne rentre pas dans le donjon")
	end
	if x==7 and y==3 then
		create_msg("panneau",
		"je suis le panneau\nd'introducion")
	end
	if x==9 and y==13 then
		create_msg("erika","bonjour n'essaye pas d'entrer\ndans le donjon ou sinon...","tu pourais avoir de mauvaise \nsurprise")
	end
	if y==25 and x>=1 and x<=5 
	and not visited_dungeon then
		create_msg("maitre du donjon",
		"tu n'aurais jamais du mettre\nles pieds ici !")
		visited_dungeon=true
	end
	if x==113 and y==1 then
		create_msg("panneau",
		"bienvenue dans le donjon")
	end
	if x==114 and y==2 and
	not visited_dungeon_nO1 then
		create_msg("donjon",
		"donjon de fer nO1")
		visited_dungeon_nO1=true
	end
	if x==8 and y==26 and
	not escalier1 then
		create_msg("maire du donjon",
		"ni rentre pas\nou sinon a tes risque peril")
		escalier1=true
	end
end

function draw_player()
	spr(p.sprite,
	p.x*8+p.ox,p.y*8+p.oy,1,1,p.flip)
end

