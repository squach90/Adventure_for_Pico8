end

function draw_gameover()
			cls(2)
			rectfill(31,53,105,79,0)
			rectfill(28,50,102,76,1)
			palt(0,false)
			palt(12,true)
			spr(32,35,55) print(p.keys,55,54,7)
			spr(33,35,65) print(p.moneys,55,67,7)
end
