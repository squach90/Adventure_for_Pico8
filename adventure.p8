	cls()
	print("press ❎ to start",30,60)
end

function update_game()
	
end

function draw_game()
	
end

_update = update_menu
_draw = draw_menu

function _init()
	create_player()
	init_msg()
	init_camera()
end

function update_game()
	if btnp(❎) then
		_update = update_game
		_draw = draw_game
	end

	if not message[1] then
		player_mouvement()
	end
	game_over()
	update_camera()
	upadte_msg()
	check_flag()
	check_flag()
	check_chest()
	
end

function draw_game()
	cls()
	print("press ❎ to start",30,60)
	draw_map()
	draw_player()
	draw_ui()
	draw_msg()
	if p.hearts==0 then
		draw_gameover()
	end
	
	
end
