--map

function draw_map()
	map(0,0,0,0,128,64)
end



function check_enemies(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

function check_moneys(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

function check_flag(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

function check_chest(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

function rnd_chest(number)
	flr(rnd(2))
end

function init_camera()
	camx,camy=0,0
end

function update_camera()
	sectionx=flr(p.x/16)*16
	sectiony=flr(p.y/16)*16
	
	destx=sectionx*8
	desty=sectiony*8
	
	diffx=destx-camx
	diffy=desty-camy
	
	diffx/=4
	diffy/=4
	
	camx+=diffx
	camy+=diffy
	
	camera(camx,camy)
end

function old_camera()
	camx=mid(0,p.x-7.5,31-15)
	camy=mid(0,p.y-7.5,31-15)
	camera(camx*8,camy*8)
end

function next_tile(x,y)
	sprite=mget(x,y)
	mset(x,y,sprite+1)
end

function pick_up_key(x,y)
	next_tile(x,y)
	p.keys+=1
	sfx(0)
end

function pick_up_hearts(x,y)
	next_title(x,y)
	p.hearts+=1
	sfx(3)
end

function open_door(x,y)
	next_tile(x,y)
	p.keys-=1
	sfx(1)
end


